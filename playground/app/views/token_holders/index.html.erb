<p id="notice"><%= notice %></p>

<h1>Token Holders</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Token</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @token_holders.each do |token_holder| %>
      <tr>
        <td><%= token_holder.name %></td>
        <td><%= token_holder.token %></td>
        <td><%= link_to 'Show', token_holder %></td>
        <td><%= link_to 'Edit', edit_token_holder_path(token_holder) %></td>
        <td><%= link_to 'Destroy', token_holder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Token Holder', new_token_holder_path %>
